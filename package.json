{
  "name": "@jupiterone/graph-template",
  "version": "0.1.0",
  "description": "A template for JupiterOne graph converters.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-template",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:containers": "docker-compose up -d",
    "stop:containers": "docker-compose down",
    "restart:containers": "docker-compose restart",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "lint": "yarn lint:es && yarn lint:md",
    "lint:md": "remark .",
    "lint:es": "eslint . --ext .ts --ignore-pattern dist",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest"
  },
  "dependencies": {
    "@jupiterone/jupiter-managed-integration-sdk": "^33.1.1",
    "@types/moment": "^2.13.0",
    "@types/node-fetch": "^2.5.4",
    "aws-sdk": "^2.619.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^8.0.1",
    "@types/gremlin": "^3.4.3",
    "@types/jest": "^25.1.2",
    "@types/node": "~13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "fs-extra": "^8.0.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-haste-map": "^25.1.0",
    "jest-resolve": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "1.19.1",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
