{
  "name": "@jupiterone/graph-airwatch",
  "version": "0.3.1",
  "description": "A graph conversion tool for https://www.air-watch.com/.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-airwatch",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepush": "yarn lint && yarn type-check && jest --changedSince master",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^3.6.0"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-core": "^3.6.0",
    "@jupiterone/integration-sdk-dev-tools": "^3.6.0",
    "@jupiterone/integration-sdk-testing": "^3.6.0",
    "@types/node-fetch": "^2.5.7"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1"
  }
}
